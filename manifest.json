{
    "manifest_version": 2,
    "name": "Focus Not",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "all_frames": true,
        "js": ["content.js"]
      }
    ],
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "browser_action": {
      "default_icon": "detour.png"
    }
  }